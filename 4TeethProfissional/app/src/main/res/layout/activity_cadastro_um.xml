<?xml version="1.0" encoding="utf-8"?>
<!---android:background="@color/gelo_esverdeado"-->
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:theme="@style/Theme.4TeethProfissional"
    tools:context=".CadastroUm">

  <ImageView
      android:id="@+id/icNavegarVoltar"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:src="@drawable/ic_navegar_voltar"
      />

  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/TvGerenciarPerfil"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/sing_up"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      android:textStyle="bold"
      android:textSize="30sp"
      android:layout_gravity="center_horizontal"
      />

  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/TvNome"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/name"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      android:textSize="20sp"

      />

  <androidx.appcompat.widget.AppCompatEditText
      android:id="@+id/EtNome"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:hint="@string/example_name"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      />

  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/TvEmail"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/email"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      android:textSize="20sp"

      />

  <androidx.appcompat.widget.AppCompatEditText
      android:id="@+id/EtEmail"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:hint="@string/example_email"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      />

  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/TvTelefone"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/fone"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      android:textSize="20sp"
      />
<!--
  <com.santalu.maskara.widget.MaskEditText
      android:id="@+id/etTelefone"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:hint="@string/example_fone_number"
      android:inputType="number"
      app:mask="+55 (__) _____-____"
      app:maskCharacter="_"
      app:maskStyle="completable"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      />
  -->
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/TvCurriculo"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/mini_curriculum"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      android:textSize="20sp"

      />

  <androidx.appcompat.widget.AppCompatEditText
      android:id="@+id/EtCurriculo"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:hint="@string/example_mini_curriculum"
      android:layout_marginTop="10dp"
      android:layout_margin="10dp"
      />

  <androidx.appcompat.widget.AppCompatButton
      android:id="@+id/btnContinuar"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:paddingLeft="10dp"
      android:paddingRight="10dp"
      android:text="@string/btnContinue"
      android:layout_gravity="center"
      android:background="@drawable/rounded_button"
      />
  </androidx.appcompat.widget.LinearLayoutCompat>